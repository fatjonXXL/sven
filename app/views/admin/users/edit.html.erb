<% content_for :title do %>
	<%= @user == current_user ? "Můj profil" : "Editace uživatele \"#{@user.login}\"" %>
<% end %>

<% form_for :user, @user, :url => admin_user_url( @user ) do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form", :object => f %>
  <%= f.submit "upravit" %>
<% end %>